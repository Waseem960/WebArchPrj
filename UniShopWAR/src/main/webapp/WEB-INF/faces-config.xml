<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<resource-bundle>
			<base-name>it.unitn.myMessages</base-name>
			<var>msgs</var>
		</resource-bundle>
		<message-bundle>it.unitn.myMessages</message-bundle>
	</application>
	<navigation-rule>
		<from-view-id>/pages/restrict/LeaveComment.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>HomePage</from-outcome>
			<to-view-id>/pages/HomePage.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/restrict/InsertShop.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>HomePage</from-outcome>
			<to-view-id>/pages/HomePage.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/restrict/ModifyUser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Profile</from-outcome>
			<to-view-id>/pages/restrict/Profile.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/restrict/Profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Profile</from-outcome>
			<to-view-id>/pages/restrict/Profile.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/restrict/ModifyShop.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>HomePage</from-outcome>
			<to-view-id>/pages/HomePage.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/InsertUser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Login</from-outcome>
			<to-view-id>/pages/Login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
